    "mocha",
    "unit",
    "testing"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^8.0.1",
    "eslint-config-google": "^0.14.0",
    "husky": "^7.0.4",
    "mocha": "^9.1.3",
    "mochawesome": "^6.3.1",
    "nyc": "^15.1.0"
  },
  "nyc": {
    "reporter": "html",
    "check-coverage": true,
    "branches": 15,
    "lines": 40,
    "functions": 40
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run coverage"
    }
  }
